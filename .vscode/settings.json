{
  "editor.tabSize": 2,
  "eslint.alwaysShowStatus": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll": true
  },
  "editor.formatOnSave": true,
  "typescript.updateImportsOnFileMove.enabled": "prompt",
  "cSpell.ignoreWords": [
    "rtadmin",
    "immer",
    "abortcontroller",
    "glyphicon",
    "toboo",
    "toboos",
    "tsbuildinfo"
  ]
}
