{
  "name": "@ovine/init",
  "version": "0.0.1-alpha.7",
  "description": "Apply ovine template more easily.",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CareyToboo/ovine.git"
  },
  "bugs": {
    "url": "https://github.com/CareyToboo/ovine/issues"
  },
  "author": "carey toboos",
  "license": "MIT",
  "bin": {
    "ovine-init": "bin/index.js"
  },
  "keywords": [
    "aims",
    "ovine",
    "react",
    "typescript",
    "admin-template"
  ],
  "scripts": {
    "tsc": "yarn clean && tsc",
    "dev": "tsc -w",
    "clean": "rimraf ./lib",
    "clean:tmp": "rimraf ./tmp/*",
    "cli": "node ../cli/bin/index.js",
    "gen:env": "rm -rf ./env/.ovine && yarn cli dll env && yarn cli scss env && sed -i '' 's/\\\"\\.\\.\\/\\./\\\"/g' ./env/.ovine/dll_manifest.json",
    "test:base:normal": "rimraf ./tmp/base_normal && node ./bin/index.js init base_normal ./tmp",
    "test:base:constraint": "rimraf ./tmp/base_constraint && node ./bin/index.js init base_constraint ./tmp",
    "test:ts:normal": "rimraf ./tmp/ts_normal && node ./bin/index.js init ts_normal ./tmp",
    "test:ts:constraint": "rimraf ./tmp/ts_constraint && node ./bin/index.js init ts_constraint ./tmp"
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "commander": "^4.0.1",
    "figlet": "^1.3.0",
    "fs-extra": "^8.1.0",
    "inquirer": "^7.0.0",
    "ora": "^4.0.3",
    "semver": "^6.3.0",
    "shelljs": "^0.8.3"
  },
  "engines": {
    "node": ">=10.9.0"
  }
}
